
lsDataSource = ProfileString( fsiniFile, lsKey, "ServerName", "-1")
(In Online Enviroment 
	ServerName=10.99.22.165
	DATABASE=VGHHMS)
lsDBName= ProfileString(fsiniFile, lsKey, "DATABASE","-1")

lsProvider ='SQLOLEDB'
//Combine string 
lsDBParm = "PROVIDER='" + lsProvider + "',DATASOURCE='"+ lsDataSource + &
		           "', PROVIDERSTRING='DataBase=" + lsDBName + "', DisableBind=0"
//Set Parameter
fSQLCA.DBMS = "OLE DB"
fSQLCA.LogId  = "osun"
fSQLCA.Logpass = "osun000"
fSQLCA.AutoCommit = FALSE
fSQLCA.DBParm = lsDBParm
fSQLCA.Lock = "RC"

Disconnect using fSQLCA;

Connect using fSQLCA;
